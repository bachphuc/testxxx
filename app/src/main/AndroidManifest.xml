<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.learn.mobile">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <permission
        android:name="com.dsoft.foxmobi.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />
    <uses-permission android:name="com.dsoft.foxmobi.permission.C2D_MESSAGE" />

    <application
        android:name=".DMobiApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.SearchActivity" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:windowSoftInputMode="adjustPan|adjustNothing">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.UserProfileActivity"
            android:label="@string/title_activity_user_profile"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Theme.DesignDemo">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
            >
        </activity>
        <activity
            android:name=".activity.UploadActivity"
            android:label="@string/title_activity_upload"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.PostActivity"
            android:theme="@style/PostActivity">
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/*" />
                <data android:mimeType="text/plain" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.FeedDetailActivity"
            android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".activity.PhotoViewerActivity"
            android:label="@string/title_activity_photo_viewer"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.SampleActivity"
            android:label="@string/title_activity_sample"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.SampleDetailActivity"
            android:label="@string/title_activity_sample_detail"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.PhotoDetailActivity"
            android:parentActivityName=".MainActivity"
            android:theme="@style/Translucent">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".activity.Sample1Activity"
            android:label="@string/title_activity_sample1"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/AppTheme" />
        <activity android:name=".activity.CommentActivity" />
        <activity
            android:name=".activity.UploadAvatarActivity"
            android:theme="@style/Translucent" />
        <activity android:name=".activity.FunnyViewer" />
        <activity android:name=".activity.ChatDetailActivity" />
        <activity android:name=".activity.BlogDetailActivity"></activity>

        <activity android:name=".customview.chromecustomtab.WebviewActivity"></activity>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <category android:name="com.example.gcm" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.learn.mobile.MyGcmListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="com.learn.mobile.MyInstanceIDListenerService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>

        <service
            android:name="com.learn.mobile.RegistrationIntentService"
            android:exported="false"></service>
    </application>

</manifest>